    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <ApplicationIcon>league of legends.ico</ApplicationIcon>
    <PackageIcon>league of legends.ico</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="league of legends.ico" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\Downloads\league of legends.ico">
